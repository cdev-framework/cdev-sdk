{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "frontend_function.json",
    "type": "object",
    "reference_name": "FRONTEND_FUNCTION",
    "required": [
        "original_path",
        "parsed_path",
        "source_code_hash",
        "dependencies_hash",
        "identity_hash",
        "metadata_hash",
        "hash",
        "timestamp",
        "local_function_name",
        "configuration",
        "needed_lines",
        "dependencies"
    ],
    "properties": {
        "original_path": {
            "$id": "#/properties/original_path",
            "type": "string"
        },
        "parsed_path": {
            "$id": "#/properties/parsed_path",
            "type": "string"
        },
        "source_code_hash": {
            "$id": "#/properties/source_code_hash",
            "type": "string"
        },
        "dependencies_hash": {
            "$id": "#/properties/dependencies_hash",
            "type": "string"
        },
        "identity_hash": {
            "$id": "#/properties/identity_hash",
            "type": "string"
        },
        "metadata_hash": {
            "$id": "#/properties/metadata_hash",
            "type": "string"
        },
        "hash": {
            "$id": "#/properties/hash",
            "type": "string"
        },
        "timestamp": {
            "$id": "#/properties/timestamp",
            "type": "string"
        },
        "local_function_name": {
            "$id": "#/properties/local_function_name",
            "type": "string"
        },
        "needed_lines": {
            "$id": "#/properties/needed_lines",
            "type": "array"
        },
        "dependencies": {
            "$id": "#/properties/dependencies",
            "type": "array"
        },  
        "configuration": {
            "anyOf": [
                {
                    "$ref": "../../backend/aws/lambda_configuration.schema.json"
                }
            ]
        }

    },
    "additionalProperties": true
}